<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/MatchMatch"
    android:background="@color/whiteColors"
    android:fitsSystemWindows="true"
    android:orientation="vertical">

    <include layout="@layout/common_title" />

    <TextView style="@style/DividerWidth" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen210"
        android:scaleType="fitXY"
        android:src="@mipmap/ic_launcher" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen38"
        android:layout_marginTop="@dimen/dimen100"
        android:gravity="center"
        android:orientation="horizontal"
        android:paddingLeft="@dimen/dimen57"
        android:paddingRight="@dimen/dimen57">

        <TextView
            android:id="@+id/tv_checkOrder"
            style="@style/AutoMatch"
            android:layout_marginRight="@dimen/dimen46"
            android:background="@drawable/shape_paycomplete"
            android:gravity="center"
            android:text="@string/checkOrder"
            android:textColor="@color/whiteColors"
            android:textSize="@dimen/textsize16" />

        <TextView
            android:id="@+id/tv_returnHomePage"
            style="@style/AutoMatch"
            android:background="@drawable/shape_paycomplete1"
            android:gravity="center"
            android:text="@string/returnHomePage"
            android:textColor="@color/greenColors"
            android:textSize="@dimen/textsize16" />

    </LinearLayout>


</LinearLayout>