    <ImageView
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen270"
        android:scaleType="fitXY"
        android:src="@mipmap/ic_launcher" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen18"
        android:background="@android:color/transparent"
        android:gravity="center"
        android:text="@string/login"
        android:textColor="@color/fFFFFFColor"
        android:textSize="@dimen/textSize17" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen255"
        android:layout_marginLeft="@dimen/dimen20"
        android:layout_marginRight="@dimen/dimen20"
        android:layout_marginTop="@dimen/dimen207"
        android:background="@drawable/shape_loginbox"
        android:orientation="vertical"
        android:paddingLeft="@dimen/dimen40"
        android:paddingRight="@dimen/dimen40">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen5"
            android:layout_marginTop="@dimen/dimen43">

            <TextView
                android:id="@+id/tv_countryCode"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="+8666"
                android:textColor="@color/bCC6CCColor"
                android:textSize="@dimen/textSize15" />

            <ImageView
                android:id="@+id/img"
                android:layout_width="@dimen/dimen7"
                android:layout_height="@dimen/dimen4"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen3"
                android:layout_toRightOf="@id/tv_countryCode"
                android:src="@mipmap/ic_launcher" />

            <TextView
                android:id="@+id/tv_divider"
                android:layout_width="@dimen/dimen0.5"
                android:layout_height="@dimen/dimen14"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen5"
                android:layout_toRightOf="@id/img"
                android:background="@color/dDDDDDColor" />

            <EditText
                android:id="@+id/et_accountNumber"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/dimen7"
                android:layout_toRightOf="@id/tv_divider"
                android:background="@android:color/transparent"
                android:hint="@string/hintAccountText"
                android:inputType="phone"
                android:maxLength="11"
                android:singleLine="true"
                android:textColor="@color/blackColor"
                android:textColorHint="@color/hintColor"
                android:textCursorDrawable="@drawable/shape_cursor"
                android:textSize="@dimen/textSize13" />

        </RelativeLayout>


        <TextView style="@style/DividerWidth" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dimen5"
            android:layout_marginTop="@dimen/dimen37"
            android:orientation="horizontal">

            <ImageView
                android:layout_width="@dimen/dimen16"
                android:layout_height="@dimen/dimen19"
                android:scaleType="fitXY"
                android:src="@mipmap/ic_launcher" />


            <EditText
                android:id="@+id/et_pwd"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/dimen50"
                android:layout_weight="1"
                android:background="@android:color/transparent"
                android:digits="@string/alphanumeric"
                android:hint="@string/hintPasswordText"
                android:inputType="textPassword"
                android:maxLength="20"
                android:singleLine="true"
                android:textColor="@color/blackColor"
                android:textColorHint="@color/hintColor"
                android:textCursorDrawable="@drawable/shape_cursor"
                android:textSize="@dimen/textSize13" />


        </LinearLayout>


        <TextView style="@style/DividerWidth" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_marginTop="@dimen/dimen19"
            android:text="@string/forgotPassword"
            android:textColor="@color/hintColor"
            android:textSize="@dimen/textSize13" />


        <TextView
            style="@style/Match44"
            android:layout_marginTop="@dimen/dimen20"
            android:background="@drawable/shape_login"
            android:gravity="center"
            android:text="@string/login"
            android:textColor="@color/fFFFFFColor"
            android:textSize="@dimen/textSize16" />


    </LinearLayout>



















